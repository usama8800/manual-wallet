{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run react:dev\" \"npm run electron:dev\"",
    "react:dev": "next dev",
    "react:build": "next build",
    "react:start": "next start",
    "react:lint": "next lint",
    "electron:dev": "nodemon",
    "electron:build": "tsc -p electron",
    "electron:start": "npm run electron:build && electron ./dist/main.js",
    "lint": "npx eslint ./src ./electron --fix",
    "qwe": "nodemon --config nodemon.random.json"
  },
  "prisma": {
    "schema": "electron/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "10.4.14",
    "bip32": "^4.0.0",
    "bitcoinjs-lib": "^6.1.1",
    "concurrently": "^8.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "ecpair": "^2.1.0",
    "electron": "^24.3.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-react-app": "^7.0.1",
    "keccak": "^3.0.1",
    "next": "13.4.2",
    "nodemon": "^2.0.22",
    "postcss": "8.4.23",
    "randombytes": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "sass": "^1.62.1",
    "secp256k1": "^5.0.0",
    "tiny-secp256k1": "^2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/keccak": "^3.0.1",
    "@types/node": "20.2.1",
    "@types/randombytes": "^2.0.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/secp256k1": "^4.0.3",
    "prisma": "^4.14.1"
  }
}
